{
    "version": 2,
    "builds": [
      { "src": "index.html", "use": "@vercel/static-build" },
      { "src": "login.html", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/statics(.*)", "dest": "/statics/$1" },
      { "src": "/styles/(.*)", "dest": "/styles/$1" },
      { "src": "/scripts/(.*)", "dest": "/scripts/$1" }
    ]
  }
  