{
    "version": 2,
    "routes": [
      {
        "src": "/pages/(.*)",
        "dest": "/pages/$1"
      },
      {
        "src": "/statics/(.*)",
        "dest": "/statics/$1"
      },
      {
        "src": "/styles/(.*)",
        "dest": "/styles/$1"
      },
      {
        "src": "/scripts/(.*)",
        "dest": "/scripts/$1"
      }
    ]
  }
  